<?xml version="1.0" encoding="utf-8"?>
<manifest>
	<application>
		<name>wap对应页面跳转</name>                  
		<alias>Medz3gUrlSkip</alias>
		<version>1.0</version>	 
		<pw-version>9.0</pw-version>       
		<description></description>   
		<logo>images/logo.jpg</logo>		
		<type>app</type>					
		<charset>GBK</charset>		 
		<website>http://blog.xiaoke.org</website>	 
		<author-name>小七</author-name>		     
		<author-email>imxieke@qq.com</author-email>		  	
		<author-icon>your icon</author-icon>	         
	</application>

	<res>res</res>

	<installation-service></installation-service>
	<inject-services>
	<s_head>
		<app_Medz3gUrlSkip>
		<class>EXT:Medz3gUrlSkip.service.srv.App_Medz3gUrlSkip_ConfigDo</class>
		<loadway>load</loadway>
		<method>getHead</method>
		<description>s_head 载入</description>
		</app_Medz3gUrlSkip>
	</s_head>
	<s_footer>
		<app_Medz3gUrlSkip>
		<class>EXT:Medz3gUrlSkip.service.srv.App_Medz3gUrlSkip_ConfigDo</class>
		<loadway>load</loadway>
		<method>getFooter</method>
		<description>s_footer 载入</description>
		</app_Medz3gUrlSkip>
	</s_footer>
</inject-services>
</manifest>
